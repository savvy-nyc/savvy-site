{
  "name": "savvy-site",
  "version": "0.0.0",
  "description": "Site boilerplate",
  "scripts": {
    "build": "next build src && ./node_modules/gulp/bin/gulp.js build",
    "build-container": "node ./node_modules/gulp/bin/gulp.js build-container",
    "dev": "APP_ENV=local node server",
    "compile": "node lib/cli/index.js",
    "create": "node lib/cli/index.js",
    "delete": "node lib/cli/index.js",
    "deploy": "node lib/cli/index.js",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "format": "prettier --write \"src/**/*.js\"",
    "local": "DEV_SERVER=true node lib/cli/local",
    "manifest": "./node_modules/gulp/bin/gulp.js codebuild",
    "codebuild": "npm run build && npm run manifest",
    "package": "node lib/cli/index.js",
    "push": "node lib/cli/index.js",
    "server": "NODE_ENV=production node dist2/server/local",
    "start": "next start src",
    "status": "node lib/cli/status",
    "test": "node lib/cli/index.js",
    "update": "node lib/cli/index.js",
    "validate": "node lib/cli/index.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spencerbeggs/savvy-site.git"
  },
  "author": "C. Spencer Beggs <spencer@savvy.nyc>",
  "license": "UNLICENCED",
